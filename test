function googleTranslateElementInit() {

  new google.translate.TranslateElement({pageLanguage: 'en', layout: google.translate.TranslateElement.InlineLayout.HORIZONTAL, gaTrack: true, gaId: 'UA-418557-5'}, 'google_translate_element');

}

function isMobile(){
    return navigator.userAgent.match('iPad') || navigator.userAgent.match('iPhone') || navigator.userAgent.match('iPod') || navigator.userAgent.match('Android');
}

(function(){
	var o = $j("div[id*='footerall']");
	if(isMobile()){
		$j("div[class='leftAdds']",o).css('display','none');
	}
})();

</script>
