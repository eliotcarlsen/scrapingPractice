<script type="text/javascript">
	function changeLookingFor(combo)
	{
		var selectedCode = combo.options[combo.selectedIndex].value;
		var delimitedValue = document.getElementById('individualAllocationPermitTypes').value;
		var result = delimitedValue.split(";");
		var debug = delimitedValue + " " + result.length +  "\n";
		var matched = false;
		for ( i=0;i<result.length; i++ )
		{
			if ( selectedCode == result[i] )
			{
				matched = true;
				break;
			}
		}
		if ( matched )
		{
			document.getElementById('permitTypeAdditionalFields').className = "hiddenoptions";
			document.getElementById('individualAllocationPermitType').value="true";
		}
		else
		{
			document.getElementById('permitTypeAdditionalFields').className = "hiddenoptions hide";
			document.getElementById('individualAllocationPermitType').value="false";
		}
	}

	function syncEntranceDistrict(entranceCombo)
	{
		var selectedEntranceComboId = entranceCombo.options[entranceCombo.selectedIndex].value;
		var result = selectedEntranceComboId.split('|');
		var districtCombo = document.getElementById('districtId');
		for ( i=0; i<districtCombo.options.length; i++ )
		{
			if ( districtCombo.options[i].value == result[1] )
			{
				//alert(districtCombo.options[i].text + " : "  + districtCombo.options[i].value );
				districtCombo.options[i].selected = true;
				break;
			}
		}
	}

	function rebuildTheTrail (){
		var permitType = document.getElementById('permitTypeId');

		if (document.getElementById('permitTypeId') == null || document.getElementById('permitTypeId') == 'undefined' ){
			return false;
		}else{
			permitType = document.getElementById('permitTypeId').value;
		}

		var entryType = '1';

		if(document.getElementById('existyes') != null && document.getElementById('existyes') != 'undefined' &&
			document.getElementById('existyes').checked){
			entryType = '2';
		}

		var date = document.getElementById('entryStartDate');

		if (document.getElementById('entryStartDate') != null && document.getElementById('entryStartDate') != 'undefined'){
			date = document.getElementById('entryStartDate').value;
		}

		var supportsExitSearch = true;
		var url = "/entranceSearch.do?action=rebuildTheTrail&permitTypeId=" + permitType ;

		var token = document.getElementById('actionToken');
		if ( token != undefined ) {
			url = "/entranceSearch.do?action=rebuildTheTrail&permitTypeId=" + permitType +"&actionToken=" + token.value;
		}

		var ajax = new Ajax.DynamicHTML(url);
		ajax.submit("&entryType=" + entryType + "&date=" + date + "&supportsExitSearch=" + supportsExitSearch);
		return false;

	}

	function setUpSelectedPermitType (){
		var permitType = getSelectedPermitType();
		var exitParam = "";
		if (permitTypeNeedsExit[permitType.selectedIndex] == 'Y') {
			exitParam = "&supportsExitSearch=true";
		}
		var url = "/entranceSearch.do?action=setpermittype";
		var ajax = new Ajax.DynamicHTML(url);
		ajax.submit(exitParam+"&permitTypeId=" + permitType);
		return false;

	}

	function getSelectedPermitType(){
		var permitType = document.getElementById('permitTypeId');
		if (document.getElementById('permitTypeId') == null || document.getElementById('permitTypeId') == 'undefined' ){
			return false;
		}else{
			permitType = document.getElementById('permitTypeId').value;
		}
		return permitType;
	}

	function permitTypeChangeHandler(parkId) {
		var contractCode = document.getElementById( "contractCode" ).value;
		var href = "/wildernessAreaDetails.do?contractCode=" + contractCode + "&parkId=" + parkId;
		var permitType = getSelectedPermitType();
		if( isForcedTripItinerary( permitType ) ){
		  href="/entranceForcedTripSearch.do?mode=submit&reset=true&contractCode=" + contractCode + "&parkId=" + parkId + "&permitTypeId=" + permitType;
		}
		document.location.href = href;
	}

	function isForcedTripItinerary( permitType ){
	   if( typeof PermitTypesMappings != "undefined" ){
	   	  return PermitTypesMappings[permitType] != null && PermitTypesMappings[permitType].forcedTripItinerary == "true";
	   }
	   return false;
	}

	$j(document).ready(function(){
		if ($j('.openLotteriesMulti').length > 0) {
			if ($j( window ).width() > 991 ) {
		        $j('.openLotteries').slick({
		            slidesToShow: 2,
		            slidesToScroll: 1,
		            autoplaySpeed: 6000,
	              dots: true,
		            variableWidth: false,
		            adaptiveHeight: false,
		            autoplay: false
		          });
			} else {
            $j('.openLotteriesWrapper').width($j( window ).width());
		        $j('.openLotteries').slick({
                dots: true,
		            variableWidth: false,
		            adaptiveHeight: false,
		            autoplay: false
		          });
			}
		}
		$j('.openLotteriesWrapper').toggle();
	  function equalizeTitleHeight() {
		    var maxHeight = 0;
		    $j('.lottitlediv').each( function(index) {
		    	var crtHeight = $j(this).height();
		    	if (crtHeight > maxHeight) {
		    		maxHeight = crtHeight;
		    	}
		    });
		    $j('.lottitlediv').css('height', maxHeight + 'px' );
	  }
	  setTimeout(equalizeTitleHeight, 1000)
	});
	$j(window).resize(function(){
		if ($j( window ).width() > 991 ) {
			$j(".openLotteriesWrapper").css("width", "650px");
		} else {
	    $j(".openLotteriesWrapper").width($j(window).width());
		}
	});
</script></div>


