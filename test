function permitTypeChanged() {
var permitTypesDD=document.getElementById('permitTypeId');
var permitTypesDescElem=document.getElementById('permitTypeDescId');
if (permitTypesDescElem && permitTypesDD) permitTypesDescElem.innerHTML=permitTypeDescriptions[permitTypesDD.selectedIndex];
var groupSizeDiv=document.getElementById('groupSizeDivId');
var stockSizeDiv=document.getElementById('stockSizeDivId');
var watercraftSizeDiv=document.getElementById('watercraftSizeDivId');
var lengthOfStayDiv=document.getElementById('lengthOfStayDivId');
var entryExitDiv=document.getElementById('entryExitDiv');
if (permitTypesDD && groupSizeDiv && permitTypeNeedsGroupSize[permitTypesDD.selectedIndex] == 'Y')
    groupSizeDiv.className='hiddenoptions';
else groupSizeDiv.className='hiddenoptions hide';
if (permitTypesDD && stockSizeDiv && permitTypeNeedsStockSize[permitTypesDD.selectedIndex] == 'Y')
    stockSizeDiv.className='hiddenoptions';
else stockSizeDiv.className='hiddenoptions hide';
if (permitTypesDD && watercraftSizeDiv && permitTypeNeedsWatercraftSize[permitTypesDD.selectedIndex] == 'Y')
    watercraftSizeDiv.className='hiddenoptions';
else watercraftSizeDiv.className='hiddenoptions hide';
if (permitTypesDD && lengthOfStayDiv && permitTypeNeedsLengthOfStay[permitTypesDD.selectedIndex] == 'Y')
    lengthOfStayDiv.className='hiddenoptions';
else lengthOfStayDiv.className='hiddenoptions hide';
var searchType = document.getElementById('searchType');
var entryRB = document.getElementById('existno');
var exitRB = document.getElementById('existyes');
document.getElementById('entrance_label').innerHTML="Permit Zone";
if (permitTypesDD && entryExitDiv) {
    searchType.value = 1;
    if (permitTypeNeedsExit[permitTypesDD.selectedIndex] == 'Y') {
       entryExitDiv.className='hiddenoptions';
       if (entryRB.checked) {
          entryRB.value = '1';
         searchType.value = 1;
document.getElementById('entrance_label').innerHTML="Entry Trail";
       }
       if (exitRB.checked) {
          exitRB.value = '2';
         searchType.value = 2;
document.getElementById('entrance_label').innerHTML="Exit Trail";
       }
    }
    else entryExitDiv.className='hiddenoptions hide';
}
return true;
}
