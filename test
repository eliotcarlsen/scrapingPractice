<script type="text/javascript">
$j(window).load(function(){
	if (parseFloat($j("#footerall").css("bottom"), 10) != 0) {
		$j("#footerall").css({bottom: "0"});
		window.setTimeout(function() {
			 $j("#footerall").fadeIn(500);
		}, 500);
	}
});
try {
$j(document).ready(function(){
	$j("#exit_disclaimer").modalPopLite({ openButton: '#exit_disclaimer_opener', closeButton: '#exit_disclaimer_closer', isModal: false, callBack:removeExitMsg});
	// Loop over the anchor tags on the page
	$j("a").each(function(){
		UWPExitLink.processLink(this);
	});

	$j("#alertWarningPopupDiv").modalPopLite({ openButton: '#alertwarningpopupopener', closeButton: '#alertWarningCloser', isModal: true });

});
} catch ( err ) {
	  // do nothing
}
function removeExitMsg(){
	clearTimeout(exitMsgTimeout);
}
function exitPopupSetup() {
	var showLeaveConfirmDialog = false;

	if (document.getElementById("checkoutLink") || document.getElementById("timerContainer"))
		showLeaveConfirmDialog = true;

	window.onbeforeunload = function() {
		if(showLeaveConfirmDialog) {
			return "The items in your shopping cart may not be available the next time you return to Recreation.gov.";
		}
	}
	$j("a").click(function(){ window.onbeforeunload = function(){}; });
	$j('button[type="submit"]').click(function(){ window.onbeforeunload = function(){}; });
}

UWPExitLink = {};
(function(exports){
	exports.processLink = function(link){
		// Get the HREF Destination
		hrefVal = $j(link).attr("href");
		// Check that we have an href
		if ( hrefVal != undefined && hrefVal != '' && hrefVal != '#' && hrefVal.length > 4){
			// Get the href start value
			startStrVal = hrefVal.substring(0, 4);
			// Check if we have a valid URL
			if ( startStrVal == 'http' ){
				// Get the HREF domain
				hrefDomain = hrefVal.match(/:\/\/(.[^/]+)/)[1];
				// Check if we are leaving the domain
				if (hrefDomain != window.location.host && hrefDomain.indexOf(".gov") < 0 && hrefDomain.indexOf(".mil") < 0 && hrefDomain.indexOf(".si.edu") < 0 && hrefDomain.indexOf(".fed.us") < 0 ){
					var targetAttr = $j(link).attr("target");
					if ( targetAttr && targetAttr === "_blank" ) {
						return;
					}
					// Remove onClick bindings
					$j(link).unbind('click');
					$j(link).attr("onClick", "");
					// Bind a click event to open the alert message
					$j(link).click(function(event) {
						var destURL = $j(link).attr("href");
						exports.buildUI(destURL);
						event.preventDefault();
					});
				}
			}
		}
	};
	exports.buildUI = function(destURL) {
		$j("#exit_disclaimer_content").html("<table cellpadding='20'><tr><td valign='middle'><h2>You are exiting the Recreation.gov website</h2><h3>Thank you for visiting our site.</h3><p>You will now be redirected to:<br /><a class='link_truncated' id='exitBox-destHref' href='" + destURL + "' >" + destURL + "</a></p><p>We hope your visit was informative and enjoyable.</p></td></tr></table>");
		$j("#exit_disclaimer_opener").trigger('click');
		w = $j(window).width();
		if( w < 500 ){
			$j("#modalPopLite-wrapper1").css("width", "80%")
			$j("#modalPopLite-wrapper1").css("left", "10%")
			$j("#exitBox-destHref").css( "width", "70%")
		}
		exitMsgTimeout = window.setTimeout(function() {
			window.location = destURL;
		}, 5000); // length of pause, in ms
		return false;
	};
	exports.bind = function(elem,url) {
		$j(elem).unbind("click.uwpclick");
		$j(elem).bind("click.uwpclick", function(e){
			return exports.buildUI(url);
		});
	};
	exports.multi = function(elem) {
		$j.each(elem, function(key,item){
				var originalLink = $j(item).attr("href");
			 	$j(item).attr("href","#");
				$j(item).unbind("click.uwpclick");
				$j(item).bind("click.uwpclick", function(e){
				return exports.buildUI(originalLink);
			});
		});
	};
})(UWPExitLink);

</script>
